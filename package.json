{
  "name": "yaitde-mock-api",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "compile": "tsc",
    "mock": "tsc && node build/main.js -m",
    "router": "tsc && node build/main.js -r",
    "console": "tsc && node build/main.js -c",
    "api": "tsc && node build/main.js -a",
    "test": "jest",
    "dckr-bld": "DOCKER_BUILDKIT=1 docker build -f ./dockerfile-npm  --secret id=NPM_TOKEN,src=npm-token.txt -t yaitdex/yaitde-agent .",
    "dckr-clean-all": "npm run dckr-clean-mock && npm run dckr-clean-console && npm run dckr-clean-router && npm run dckr-clean-api",
    "dckr-mock-clean": "docker stop yaitde-agent-mock && docker rm yaitde-agent-mock",
    "dckr-console-clean": "docker stop yaitde-agent-console && docker rm yaitde-agent-console",
    "dckr-router-clean": "docker stop yaitde-agent-router && docker rm yaitde-agent-router",
    "dckr-api-clean": "docker stop yaitde-agent-api && docker rm yaitde-agent-api",
    "dckr-mock": "docker run -ti -p 3003:3000 -v ~/code/yaitde-test-repo:/usr/src/app/yaitde-repo -v ~/.yaitde:/usr/src/yaitde --name=yaitde-agent-mock --restart=always -e SIGNING_KEY='devsDontNeedNoStinkinKey' -e RUN_MODE='-m' yaitdex/yaitde-agent",
    "dckr-console": "docker run -ti -p 3001:3000 -v ~/code/yaitde-test-repo:/usr/src/app/yaitde-repo -v ~/.yaitde:/usr/src/yaitde --name=yaitde-agent-console --restart=always -e SIGNING_KEY='devsDontNeedNoStinkinKey' -e RUN_MODE='-c' yaitdex/yaitde-agent",
    "dckr-router": "docker run -ti -p 3000:3000 -v ~/.yaitde:/usr/src/yaitde -v /var/run/docker.sock:/var/run/docker.sock --name=yaitde-agent-router --restart=always -e SIGNING_KEY='devsDontNeedNoStinkinKey' -e RUN_MODE='-r' yaitdex/yaitde-agent",
    "dckr-api": "docker run -ti -p 3002:3000 -v ~/code/yaitde-test-repo:/usr/src/app/yaitde-repo -v ~/.yaitde:/usr/src/yaitde --name=yaitde-agent-api --restart=always -e SIGNING_KEY='devsDontNeedNoStinkinKey' -e RUN_MODE='-a' yaitdex/yaitde-agent"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@yaitde/core-lib": "^1.0.5",
    "await-spawn": "^4.0.1",
    "axios": "^0.20.0",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "cli-highlight": "^2.1.4",
    "commander": "^6.1.0",
    "desm": "^1.0.0",
    "fastify": "^3.5.1",
    "fastify-autoload": "^3.3.0",
    "fastify-cookie": "^4.1.0",
    "fastify-formbody": "^5.0.0",
    "fastify-jwt": "^2.1.3",
    "fastify-oas": "^3.0.3",
    "fastify-swagger": "^3.4.0",
    "line-reader": "^0.4.0",
    "memory-cache": "^0.2.0",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "prompt-sync": "^4.2.0",
    "prompt-sync-history": "^1.0.1",
    "random": "^2.2.0",
    "raw-body": "^2.4.1",
    "sleep-promise": "^9.0.0",
    "uuid": "^8.3.1",
    "widdershins": "^4.0.1"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "dependency-cruiser": "^9.15.0",
    "eslint": "^7.14.0",
    "jest": "^26.5.3",
    "jest-extended": "^0.11.5",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
